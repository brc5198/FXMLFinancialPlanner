                        <Label text="Last Name" />
                        <TextField fx:id="lastNameInput">
                           <VBox.margin>
                              <Insets right="20.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
                  <Button fx:id="newLogin" mnemonicParsing="false" onAction="#handleNewLogin" prefHeight="50.0" prefWidth="100.0" text="Go">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
